{
    "containerDefinitions": [
        {
            "name": "judge0-server",
            "image": "judge0/judge0:latest",
            "cpu": 0,
            "memoryReservation": 256,
            "portMappings": [
                {
                    "containerPort": 2358,
                    "hostPort": 2358,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [],
            "mountPoints": [
                {
                    "sourceVolume": "server-worker-volume",
                    "containerPath": "/data"
                }
            ],
            "volumesFrom": [],
            "dependsOn": [
                {
                    "containerName": "db",
                    "condition": "START"
                },
                {
                    "containerName": "judge0-redis",
                    "condition": "START"
                }
            ],
            "privileged": true,
            "readonlyRootFilesystem": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/code-complexity-task",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://0.0.0.0:2358/workers"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 5,
                "startPeriod": 30
            }
        },
        {
            "name": "db",
            "image": "postgres:13.0",
            "cpu": 0,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 5432,
                    "hostPort": 5432,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "YourPasswordHere1234"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "redis"
                },
                {
                    "name": "REDIS_PASSWORD",
                    "value": "YourPasswordHere1234"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_HOST",
                    "value": "db"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "postgres-db-volume",
                    "containerPath": "/data"
                }
            ],
            "volumesFrom": [],
            "privileged": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/code-complexity-task",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "pg_isready -U postgres"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 5,
                "startPeriod": 30
            }
        },
        {
            "name": "judge0-redis",
            "image": "redis:6.0",
            "cpu": 0,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [
                "bash",
                "-c",
                "docker-entrypoint.sh --appendonly yes"
            ],
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "judge0"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "YourPasswordHere1234"
                },
                {
                    "name": "REDIS_HOST",
                    "value": "redis"
                },
                {
                    "name": "REDIS_PASSWORD",
                    "value": "YourPasswordHere1234"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "judge0"
                },
                {
                    "name": "POSTGRES_HOST",
                    "value": "db"
                }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "redis-volume",
                    "containerPath": "/data"
                }
            ],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/code-complexity-task",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "redis-cli ping"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 5,
                "startPeriod": 30
            }
        },
        {
            "name": "python-server",
            "image": "public.ecr.aws/a4q2d8e1/code-complexity-backend",
            "cpu": 0,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 7000,
                    "hostPort": 7000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [
                {
                    "name": "is_dockerised",
                    "value": "True"
                },
                {
                    "name": "DISCORD_URL",
                    "value": "replace-from-secrets"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/code-complexity-task",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://0.0.0.0:7000/healthy"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 5,
                "startPeriod": 30
            }
        }
    ],
    "family": "code-complexity-task",
    "networkMode": "host",
    "volumes": [
        {
            "name": "server-worker-volume",
            "host": {
                "sourcePath": "server-worker-dir"
            }
        },
        {
            "name": "postgres-db-volume",
            "host": {
                "sourcePath": "postgres-db-dir"
            }
        },
        {
            "name": "redis-volume",
            "host": {
                "sourcePath": "redis-dir"
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ]
}